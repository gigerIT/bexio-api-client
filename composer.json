{
  "name": "gigerit/bexio-api-client",
  "type": "library",
  "description": "A PHP client for the Bexio API",
  "version": "1.1.0",
  "keywords": [
    "bexio"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Bexio\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Manuel Giger",
      "email": "info@gigeirt.ch"
    }
  ],
  "require": {
    "saloonphp/saloon": "^3.0",
    "illuminate/container": "^11.9",
    "spatie/laravel-data": "^4.6",
    "gigerit/spatie-laravel-data-standalone": "^1.0"
  },
  "require-dev": {
    "pestphp/pest": "^2.34",
    "pestphp/pest-plugin-faker": "^2.0",
    "symfony/var-dumper": "^7.1"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php check-dependency.php"
    ],
    "post-update-cmd": [
      "php check-dependency.php"
    ]
  }
}
